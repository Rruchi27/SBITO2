abstract class Product
{
   abstract void Ptype();
   abstract void Pid();
}
class Airconditioner extends Product
{
    int cost;

    public Airconditioner(int cost)
    {
        super();
        this.cost=cost;
    }

    void Ptype()
    {
        System.out.println("Product type is Split AC");
    }

    void Pid()
    {   
        int ProdID;
        System.out.println("Product id is" +ProdID);
    }

}
